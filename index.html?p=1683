					    	$("#iframelazyMap").attr("src", "https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d5897.710275440397!2d-71.08696732275394!3d42.34561177494112!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x89e37a1a19923c97%3A0x185cab9bb0d968fc!2sOakleaf+Cakes!5e0!3m2!1sen!2sus!4v1401340804333");
			            });
			        });
		        
				
				    if ($('#sticky-anchor').length){
						$(function () {
							function sticky_relocate() {
							    var window_top = $(window).scrollTop();
							    var div_top = $('#sticky-anchor').offset().top;
							    if (window_top >= div_top) {
							        $('.sticky-side').addClass('stick');
							    } else {
							        $('.sticky-side').removeClass('stick');
							    }
							}

						    $(window).scroll(sticky_relocate);
						    sticky_relocate();
						});
				
				    }
				});
				
			  </script>
	</body>

